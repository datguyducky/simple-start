{
  "manifest_version": 3,
  "name": "Simple Start",
  "version": "2.2.0",
  "description": "Highly customizable new tab extension with categorized bookmark groups",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "options_ui": {
    "page": "src/pages/settings/index.html",
    "open_in_tab": true
  },
  "chrome_url_overrides": {
    "newtab": "src/pages/newtab/index.html"
  },
  "icons": {
    "48": "icon48.png",
    "96": "icon96.png"
  }
}