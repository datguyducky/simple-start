{
    "manifest_version": 2,
    "name": "Simple Start",
    "version": "1.0.1",
    "description": "Minimalistic new tab page extension. Organize bookmark in to tags - be more productive with seperated tags for your home, work, and your hobbies. ",
    "developer": {
      "name": "datguysheepy",
      "url": "simplestart.mtymon.me"
    },

    "icons": {
      "48": "icon48.png",
      "96": "icon96.png"
    },

    "permissions": ["bookmarks", "storage"],

    "chrome_url_overrides": {
        "newtab": "simplestart.html"
    },
    "chrome_settings_overrides": {
        "homepage": "simplestart.html"
    }
  }