{
    "manifest_version": 2,
    "name": "Simple Start",
    "version": "1.0.1",
    "description": "Minimalistic new tage page extensions. Let's you to have all the bookmarks you may need every time you open new tab.",
    "developer": {
      "name": "datguysheepy",
      "url": "simplestart.mtymon.me"
    },

    "icons": {
      "48": "icon48.png",
      "96": "icon96.png"
    },

    "permissions": ["bookmarks", "storage"],

    "chrome_url_overrides": {
        "newtab": "simplestart.html"
    },
    "chrome_settings_overrides": {
        "homepage": "simplestart.html"
    }
  }